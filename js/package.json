{
  "name": "jupyter-webrtc",
  "version": "0.5.0",
  "description": "WebRTC for Jupyter notebook/lab",
  "author": "Maarten Breddels",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maartenbreddels/ipywebrtc.git"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../share/jupyter/labextensions/jupyter-webrtc",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "install-test": "npm install && karma start --single-run",
    "build": "jlpm run build:js && jlpm run build:labextension && jlpm runbuild:nbextension",
    "build:js": "tsc",
    "build:labextension": "jupyter labextension build .",
    "build:nbextension": "webpack --mode=production",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "tsc --watch",
    "watch:nbextension": "webpack --watch --mode=development",
    "prepare": "jlpm run build",
    "test": "karma start --single-run"
  },
  "devDependencies": {
    "@jupyter-widgets/controls": "^1.4.0 || ^2 || ^3",
    "@jupyterlab/services": "^2.0.3",
    "@jupyterlab/builder": "^3.0.0",
    "@types/chai": "^4.1.4",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.4",
    "@types/sinon": "^5.0.1",
    "arraybuffer-loader": "^1.0.6",
    "base64-image-loader": "^1.2.1",
    "chai": "^4.1.2",
    "json-loader": "^0.5.4",
    "karma": "^2.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.4.1",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "typescript": "^3.5.2",
    "webpack": "^5",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0 || ^2.0 || ^3.0",
    "html2canvas": "v1.0.0-alpha.12",
    "mqtt": "^2.11.0",
    "underscore": "^1.8.3",
    "webrtc-adapter": "^4.2.2"
  },
  "files": [
    "dist/",
    "css/",
    "src/",
    "lib/"
  ]
}
